n            <button class=\"buy-button\" @click=\"addToCart(game)\">加入购物车</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <router-view></router-view>\r\n    <div v-if=\"currentGame\" class=\"game-description\">\r\n      <h3>{{ currentGame.title }}</h3>\r\n      <p>{{ currentGame.description }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    /**\r\n     * @typedef {Object} Game\r\n     * @property {string} title - 游戏标题\r\n     * @property {string} type - 游戏类型\r\n     * @property {string} image - 游戏图片路径\r\n     * @property {number} originalPrice - 原价\r\n     * @property {number} discountPrice - 折后价\r\n     * @property {number} discount - 折扣百分比\r\n     * @property {string} description - 游戏描述\r\n     */\r\n\r\n    /** @type {Game[]} */\r\n    const games = [\r\n      { \r\n        title: '游戏1', \r\n        type: '动作冒险', \r\n        image: require('@/assets/gta5.jpg') ,\r\n        originalPrice: 60, \r\n        discountPrice: 48, \r\n        discount: 20,\r\n        description: '这是一款充满刺激的动作冒险游戏，适合喜欢探险和战斗的玩家。'\r\n      },\r\n      { \r\n        title: '游戏2', \r\n        type: '角色扮演', \r\n        image: 'path/to/game2-image.jpg', \r\n        originalPrice: 45, \r\n        discountPrice: 36, \r\n        discount: 20,\r\n        description: '这是一款深度的角色扮演游戏，玩家可以在游戏中自由探索和成长。'\r\n      },\r\n      { \r\n        title: '游戏3', \r\n        type: '射击', \r\n        image: 'path/to/game3-image.jpg', \r\n        originalPrice: 75, \r\n        discountPrice: 60, \r\n        discount: 20,\r\n        description: '这是一款紧张刺激的射击游戏，玩家需要精准射击击败敌人。'\r\n      },\r\n      // 添加更多游戏...\r\n    ];\r\n\r\n    /** @type {Game | null} */\r\n    let currentGame = null;\r\n\r\n    return {\r\n      games,\r\n      currentGame\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     * 显示游戏描述\r\n     * @param {Game} game - 游戏对象\r\n     */\r\n    showDescription(game) {\r\n      this.currentGame = game;\r\n    },\r\n    /**\r\n     * 隐藏游戏描述\r\n     */\r\n    hideDescription() {\r\n      this.currentGame = null;\r\n    },\r\n    /**\r\n     * 将游戏添加到购物车\r\n     * @param {Game} game - 游戏对象\r\n     */\r\n    addToCart(game) {\r\n      // 这里添加将游戏添加到购物车的逻辑\r\n      console.log(`将 ${game.title} 加入购物车`);\r\n      // 例如：this.$store.commit('addToCart', game);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.background {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: hwb(0 100% 0%);\r\n  background-size: cover;\r\n  background-position: center;\r\n  z-index: -1;\r\n}\r\n\r\n.logo-container {\r\n  text-align: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.logo {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100px;\r\n  height: auto;\r\n  text-align: center;\r\n}\r\n\r\n.logo-text {\r\n  margin: 0px;\r\n  font-size: 2.5em;\r\n  margin-top: -40px;\r\n}\r\n\r\n.header-banner {\r\n  text-align: center;\r\n  padding: 50px 0;\r\n}\r\n\r\n.header-banner img {\r\n  max-width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.navbar {\r\n  text-align: center;\r\n  margin-top: 0px;\r\n  position: relative;\r\n}\r\n\r\n.navbar ul {\r\n  list-style-type: none;\r\n  padding: 0px;\r\n}\r\n\r\n.navbar li {\r\n  display: inline-block;\r\n  margin: 0 50px;\r\n}\r\n\r\n.navbar a {\r\n  color: rgb(0, 0, 0);\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n  font-size: 1.5em;\r\n}\r\n\r\n.navbar a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.game-list {\r\n  padding: 20px;\r\n  flex-grow: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  width: 100%;\r\n}\r\n\r\n.navbar-container {\r\n  background: linear-gradient(to top, rgba(255, 255, 255, 0), rgba(0, 123, 255, 0.7)); /* 修改背景颜色为渐变 */\r\n  padding: 0px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  margin: 100px 200px 100px 200px;\r\n}\r\n\r\n.navbar .game-list {\r\n  padding: 20px;\r\n  border-top: 1px solid #000000;\r\n  margin-top: 20px;\r\n}\r\n\r\n.game-item {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 10px;\r\n  background-color: rgba(255, 255, 255, 0.9); /* 调整背景颜色以提高可读性 */\r\n  padding: 10px;\r\n  border-radius: 8px;\r\n  transition: transform 0.2s ease;\r\n  width: 800px; /* 设置固定宽度 */\r\n}\r\n\r\n.game-item:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.game-image-container {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-right: 20px;\r\n}\r\n\r\n.game-image {\r\n  max-width: 100px;\r\n  height: auto;\r\n}\r\n\r\n.game-info {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  flex-grow: 1;\r\n  width: 100%;\r\n}\r\n\r\n.game-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.game-title {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n}\r\n\r\n.game-type {\r\n  font-size: 14px;\r\n  color: #7f8c8d;\r\n}\r\n\r\n.game-prices {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.original-price {\r\n  font-size: 14px;\r\n  color: #e74c3c;\r\n  text-decoration: line-through;\r\n}\r\n\r\n.discount-price {\r\n  font-size: 16px;\r\n  color: #2ecc71;\r\n}\r\n\r\n.game-actions {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.discount {\r\n  font-size: 14px;\r\n  color: #3498db;\r\n  margin-right: 10px;\r\n}\r\n\r\n.buy-button {\r\n  background-color: #e74c3c;\r\n  color: rgb(99, 4, 4);\r\n  border: none;\r\n  padding: 5px 10px;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.buy-button:hover {\r\n  background-color: #c0392b;\r\n}\r\n\r\n.game-description {\r\n  position: fixed;\r\n  right: 20px;\r\n  top: 100px;\r\n  width: 300px;\r\n  backgro