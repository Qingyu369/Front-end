n type=\"submit\">注册</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n    <router-view></router-view>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'HomePage',\r\n  data() {\r\n    return {\r\n      username: '',\r\n      password: '',\r\n      confirmPassword: '',\r\n      showError: false,\r\n      users: [\r\n        { name: '1', role: 'user', password: '1' },\r\n        { name: '11', role: 'admin', password: '11' }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    handleRegister() {\r\n      if (this.password !== this.confirmPassword) {\r\n        this.showError = true;\r\n        return;\r\n      }\r\n\r\n      // 模拟注册成功\r\n      alert('注册成功');\r\n      this.$router.push('/UserLogin');\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.home-page {\r\n  position: relative;\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.logo-container {\r\n  text-align: center;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n}\r\n\r\n.logo {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100px;\r\n  height: auto;\r\n  text-align: center;\r\n}\r\n\r\n.logo-text {\r\n  margin: 0px;\r\n  font-size: 2.5em;\r\n  margin-top: -40px;\r\n  color: #ffffff;\r\n}\r\n\r\n.login-page {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: center;\r\n  margin: 140px 200px;\r\n  padding: 10px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  background-color: #f0f0f0;\r\n}\r\n\r\n.login-container {\r\n  background-color: #fff;\r\n  padding: 40px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  width: 400px;\r\n  z-index: 10; /* 确保登录框在背景图片之上 */\r\n  position: relative;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\n